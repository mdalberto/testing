<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="93" id="com.ionicframework.pushnotifications" ios-CFBundleVersion="0.9.3" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>PsychicSource</name>
    <description>
    Psychic Source mobile app 
  </description>
    <author email="development@novotorium.com" href="http://novotorium.com/">
    Novotorium Team
  </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="auto-hide-splash-screen" value="false" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="SplashMaintainAspectRatio" value="true|false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="3000" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="android-build-tool" value="gradle" />
    <gap:plugin name="cordova-plugin-splashscreen" source="npm" />
    <gap:plugin name="org.apache.cordova.inappbrowser" source="npm" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="phonegap-plugin-push" source="npm" version="1.4.5" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <platform name="browser">
        <preference name="SplashScreen" value="images/browser/splashscreen.jpg" />
        <preference name="SplashScreenDelay" value="3000" />
        <preference name="SplashScreenBackgroundColor" value="green" />
        <preference name="ShowSplashScreen" value="false" />
        <preference name="SplashScreenWidth" value="600" />
        <preference name="SplashScreenHeight" value="300" />
    </platform>
    <platform name="ios">
        <icon gap:platform="ios" height="180" src="icons/ios/icon-60@3x.png" width="180" />
        <icon gap:platform="ios" height="60" src="icons/ios/icon-60.png" width="60" />
        <icon gap:platform="ios" height="120" src="icons/ios/icon-60@2x.png" width="120" />
        <icon gap:platform="ios" height="76" src="icons/ios/icon-76.png" width="76" />
        <icon gap:platform="ios" height="152" src="icons/ios/icon-76@2x.png" width="152" />
        <icon gap:platform="ios" height="29" src="icons/ios/icon-Small.png" width="29" />
        <icon gap:platform="ios" height="58" src="icons/ios/icon-Small@2x.png" width="58" />
        <icon gap:platform="ios" height="87" src="icons/ios/icon-Small@3x.png" width="87" />
        <icon gap:platform="ios" height="40" src="icons/ios/icon-Small-40.png" width="40" />
        <icon gap:platform="ios" height="80" src="icons/ios/icon-Small-40@2x.png" width="80" />
        <splash gap:platform="ios" height="480" src="splashscreen/ios/Default.png" width="320" />
        <splash gap:platform="ios" height="960" src="splashscreen/ios/Default@2x.png" width="640" />
        <splash gap:platform="ios" height="1136" src="splashscreen/ios/Default-568h@2x.png" width="640" />
        <splash gap:platform="ios" height="1334" src="splashscreen/ios/Default-667h@2x.png" width="750" />
        <splash gap:platform="ios" height="2208" src="splashscreen/ios/Default-Portrait-736h@3x.png" width="1242" />
        <splash gap:platform="ios" height="1024" src="splashscreen/ios/Default-Portrait.png" width="768" />
        <splash gap:platform="ios" height="768" src="splashscreen/ios/Default-Landscape.png" width="1024" />
        <splash gap:platform="ios" height="2048" src="splashscreen/ios/Default-Portrait@2x.png" width="1536" />
        <splash gap:platform="ios" height="1536" src="splashscreen/ios/Default-Landscape@2x.png" width="2048" />
    </platform>
    <platform name="android">
        <icon gap:density="ldpi" src="icons/android/ldpi/psychic-source-icon.png" />
        <icon gap:density="mdpi" src="icons/android/mipmap-mdpi/psychic-source-icon.png" />
        <icon gap:density="hdpi" src="icons/android/mipmap-hdpi/psychic-source-icon.png" />
        <icon gap:density="xhdpi" src="icons/android/mipmap-xhdpi/psychic-source-icon.png" />
        <icon gap:density="xxhdpi" src="icons/android/mipmap-xxhdpi/psychic-source-icon.png" />
        <icon gap:density="xxxhdpi" src="icons/android/mipmap-xxxhdpi/psychic-source-icon.png" />
        <splash gap:qualifier="land-ldpi" src="splashscreen/android/splash-land-LDPI.png" />
        <splash gap:qualifier="land-mdpi" src="splashscreen/android/splash-land-MDPI.png" />
        <splash gap:qualifier="land-hdpi" src="splashscreen/android/splash-land-HDPI.png" />
        <splash gap:qualifier="land-xhdpi" src="splashscreen/android/splash-land-XHDPI.png" />
        <splash gap:qualifier="land-xxhdpi" src="splashscreen/android/splash-land-XXHDPI.png" />
        <splash gap:qualifier="land-xxxhdpi" src="splashscreen/android/splash-land-XXXHDPI.png" />
        <splash gap:qualifier="port-ldpi" src="splashscreen/android/splash-port-LDPI.png" />
        <splash gap:qualifier="port-mdpi" src="splashscreen/android/splash-port-MDPI.png" />
        <splash gap:qualifier="port-hdpi" src="splashscreen/android/splash-port-HDPI.png" />
        <splash gap:qualifier="port-xhdpi" src="splashscreen/android/splash-port-XHDPI.png" />
        <splash gap:qualifier="port-xxhdpi" src="splashscreen/android/splash-port-XXHDPI.png" />
        <splash gap:qualifier="port-xxxhdpi" src="splashscreen/android/splash-port-XXXHDPI.png" />
        <splash src="screen.png" />
    </platform>
    <plugin name="ionic-plugin-keyboard" spec="~2.1.0" />
</widget>
